{
  "name": "@formsort/react-embed",
  "version": "2.1.0",
  "description": "Embed formsort flows in react components",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "format": "eslint --ext .ts,.tsx src --fix",
    "lint": "eslint --ext .ts,.tsx src",
    "pack": "yarn pack",
    "release": "craft prepare --publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formsort/oss.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "formsort",
    "embed",
    "react"
  ],
  "author": "Ali Kirecligol <ali@formsort.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/formsort/oss/packages/react-embed/issues"
  },
  "homepage": "https://github.com/formsort/oss/packages/react-embed#readme",
  "devDependencies": {
    "@formsort/eslint-config": "^1.3.7",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^27.0.1",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^27.1.0",
    "prettier": "^2.2.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@formsort/web-embed-api": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^16.13.0 || ^17.0.0"
  },
  "jest": {
    "cacheDirectory": "./.jest-cache",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "jsdom"
  }
}

{
  "name": "@formsort/web-embed-api",
  "version": "2.2.2",
  "description": "Embed Formsort flows within other webpages",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc --project tsconfig.build.json",
    "format": "eslint --ext .ts,.tsx src --fix",
    "lint": "eslint --ext .ts,.tsx src",
    "pack": "yarn pack",
    "release": "craft prepare --publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formsort/oss.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "forms",
    "formsort",
    "embed"
  ],
  "author": "Formsort Engineering <engineering@formsort.com>",
  "license": "MIT",
  "devDependencies": {
    "@formsort/eslint-config": "^1.3.7",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^27.1.0",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@formsort/constants": "^1.6.0"
  },
  "jest": {
    "cacheDirectory": "./.jest-cache",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "jsdom"
  }
}

{
  "name": "@formsort/web-embed-api",
  "version": "2.1.0",
  "description": "Embed Formsort flows within other webpages",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "format": "eslint -c .eslintrc.js --ext .ts,.tsx src --fix",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx src",
    "pretty-check": "prettier --check src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formsort/oss.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "forms",
    "formsort",
    "embed"
  ],
  "author": "Fil Zembowicz <fil@formsort.com>",
  "license": "MIT",
  "devDependencies": {
    "@formsort/eslint-config": "^1.3.7",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^27.1.0",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@formsort/constants": "^1.5.0"
  }
